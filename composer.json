{
  "name": "wodby/wordpress-composer",
  "description": "Project template for WordPress projects with composer",
  "type": "project",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://connect.advancedcustomfields.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal-composer/preserve-paths": true,
      "johnpbloch/wordpress-core-installer": true
    }
  },
  "require": {
    "composer/installers": "^1.2",
    "drupal-composer/preserve-paths": "^0.1",
    "johnpbloch/wordpress-core": "~6.0",
    "johnpbloch/wordpress-core-installer": "^2.0",
    "wpackagist-plugin/contact-form-7": "^5.8",
    "wpackagist-plugin/contact-form-7-skins": "^2.6",
    "wpackagist-plugin/copy-delete-posts": "^1.4",
    "wpackagist-plugin/show-current-template": "^0.5.2",
    "wpengine/advanced-custom-fields-pro": "^6.2"
  },
  "extra": {
    "installer-paths": {
      "web/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    },
    "preserve-paths": [
      "web/wp-config.php",
      "web/wp-content/uploads",
      "web/wp-content/themes/rankexperts"
    ],
    "wordpress-install-dir": "web"
  }
}
